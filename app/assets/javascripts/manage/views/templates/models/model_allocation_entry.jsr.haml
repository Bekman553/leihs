.row.line.font-size-xs{:"data-type" => "inline-entry", :"data-new" => true}
  %input{:type => :hidden, :name => "template[model_links_attributes][][model_id]", :value => "{{>id}}"}
  .line-col.col2of6.no-padding
    .line-col.col1of2{:"data-quantities" => true}
      %input.width-full.small.text-align-center{type: "text", value: 1, min: 1, max: "{{>availability().total_rentable}}", name: "template[model_links_attributes][][quantity]", autocomplete: :off}
    .line-col.col1of2.padding-left-xs.text-align-left{:"data-quantities" => true}
      \/ {{>availability().total_rentable}}
  .line-col.col3of6.text-align-left{:"data-model-name" => true}
    {{>name()}}
  .line-col.col1of6.text-align-right
    %button.button.inset.small{:"data-remove" => true} {{jed "Remove"/}}
